  },
  "license": "MIT",
  "dependencies": {
    "pg": "^4.4.1",
    "pg-format": "^1.0.2",
    "promised-retry": "^0.2.1"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "index.js",
  "scripts": {
